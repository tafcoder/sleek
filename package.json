  "keywords": [ "css", "sass", "bootstrap", "admin", "dashboard", "responsive", "front-end", "admin-dashboard", "bootstrap4", "html", "theme", "dashboard-templates", "admin-panel", "admin-template", "uikit", "bootstrap-theme", "bootstrap-template", "admin", "bootstrap-admin-dashboard", "jekyll" ],
  "scripts": {
    "start": "npm run serve",
    "serve": "bundle exec jekyll serve",
    "build": "bundle exec jekyll build",
